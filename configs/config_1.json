{
    "application": {
        "name": "AutoWeave Enterprise Edition",
        "version": "2.5.0-beta.4",
        "buildNumber": 2024051501,
        "environment": "production",
        "debugMode": false,
        "maintenanceMode": false,
        "releaseDate": "2024-05-15T10:00:00Z",
        "supportContact": "support@autoweave.io",
        "documentationUrl": "https://docs.autoweave.io/v2.5"
    },
    "server": {
        "host": "0.0.0.0",
        "port": 8080,
        "ssl": {
            "enabled": true,
            "certPath": "/etc/ssl/certs/autoweave.crt",
            "keyPath": "/etc/ssl/private/autoweave.key",
            "caPath": "/etc/ssl/certs/ca-certificates.crt",
            "verifyClient": false
        },
        "workers": 4,
        "timeout": 60000,
        "keepAliveTimeout": 5000,
        "maxPayloadSize": "50mb",
        "cors": {
            "enabled": true,
            "origin": [
                "https://app.autoweave.io",
                "https://admin.autoweave.io",
                "https://partner-portal.autoweave.io"
            ],
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": [
                "Content-Type",
                "Authorization",
                "X-Requested-With",
                "Accept"
            ],
            "credentials": true,
            "maxAge": 86400
        },
        "rateLimit": {
            "windowMs": 900000,
            "max": 1000,
            "message": "Too many requests from this IP, please try again later."
        }
    },
    "localization": {
        "defaultLocale": "en-US",
        "supportedLocales": [
            "en-US",
            "es-ES",
            "fr-FR",
            "de-DE",
            "ja-JP",
            "zh-CN",
            "pt-BR"
        ],
        "fallbackLocale": "en-US",
        "timezone": "UTC",
        "currency": "USD",
        "dateFormat": "YYYY-MM-DD",
        "timeFormat": "HH:mm:ss"
    },
    "legal": {
        "termsOfServiceUrl": "https://autoweave.io/terms",
        "privacyPolicyUrl": "https://autoweave.io/privacy",
        "cookiePolicyUrl": "https://autoweave.io/cookies",
        "gdprCompliant": true,
        "ccpaCompliant": true
    }
}