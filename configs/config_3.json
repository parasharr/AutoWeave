{
    "features": {
        "userManagement": {
            "registration": {
                "enabled": true,
                "emailVerificationRequired": true,
                "allowSocialLogin": true,
                "providers": [
                    "google",
                    "github",
                    "microsoft",
                    "twitter"
                ],
                "maxUsersPerOrganization": 50,
                "domainsWaitlist": [
                    "example.com",
                    "test.com"
                ]
            },
            "profiles": {
                "avatars": true,
                "customFields": false,
                "bioLength": 500,
                "visibility": "public"
            },
            "2fa": {
                "enabled": true,
                "enforced": false,
                "methods": [
                    "app",
                    "sms",
                    "email"
                ]
            }
        },
        "billing": {
            "enabled": true,
            "provider": "stripe",
            "publicKey": "pk_test_1234567890abcdef",
            "webhookSecret": "whsec_1234567890abcdef",
            "plans": {
                "free": {
                    "id": "price_free_tier",
                    "name": "Starter",
                    "limits": {
                        "projects": 3,
                        "collaborators": 1,
                        "storage": "1GB"
                    }
                },
                "pro": {
                    "id": "price_pro_tier",
                    "name": "Professional",
                    "limits": {
                        "projects": 20,
                        "collaborators": 5,
                        "storage": "100GB"
                    }
                },
                "enterprise": {
                    "id": "price_ent_tier",
                    "name": "Enterprise",
                    "limits": {
                        "projects": -1,
                        "collaborators": -1,
                        "storage": "5TB"
                    }
                }
            },
            "trialDays": 14,
            "gracePeriodDays": 3
        },
        "newUI": {
            "enabled": true,
            "rolloutPercentage": 25,
            "betaUsersOnly": true,
            "feedbackWidget": true
        },
        "integrations": {
            "slack": true,
            "discord": true,
            "jira": false,
            "trello": true,
            "asana": false,
            "zoom": true
        },
        "experimental": {
            "aiAssistant": {
                "enabled": true,
                "model": "gpt-4-turbo",
                "temperature": 0.7,
                "maxTokens": 2048,
                "systemPrompt": "You are a helpful assistant for AutoWeave..."
            },
            "realtimeCollaboration": {
                "enabled": false,
                "backend": "websocket",
                "syncInterval": 500
            },
            "darkerMode": true
        }
    }
}